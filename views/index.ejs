  <style>
    html, body {
      height: 100%;
      width: 100%;
      margin: 0;
      padding: 0;
    }
    .weather {
      align-items: center;
      display: flex;
      height: 100vh;
      justify-content: center;
    }
    .weather svg {
      height: 30vw;
      width: 30vw;
    }
    .weather--sunny {
      background: radial-gradient( circle at center center, #ffbf34, #ffd270 75%, #ffe4a9);
    }
    .weather--cloudy {
      background: radial-gradient( circle at center center, #77969e, #92b6b1 75%, #c4dfdd);
    }
    .weather--rainy {
      background: radial-gradient( circle at center center, #2b91b8, #37b3ce 75%, #288ecc);
    }
    .climacon_iconWrap-cloud {

    animation-name: translateWind;
    -webkit-animation-duration: 6s;
    animation-duration: 6s;
    -webkit-animation-direction: alternate;
    animation-direction: alternate;
    -webkit-animation-timing-function: ease-in-out;
    animation-timing-function: ease-in-out;
    }
    .climacon_iconWrap-cloud-fill {
      animation-delay: 3s;
    }
  </style>
</head>
